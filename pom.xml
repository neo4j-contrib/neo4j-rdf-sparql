<?xml version="1.0" encoding="UTF-8"?>
<project>
	<parent>
		<groupId>org.neo4j</groupId>
		<artifactId>parent-pom</artifactId>
		<version>1</version>
	</parent>

	<modelVersion>4.0.0</modelVersion>
	<groupId>org.neo4j</groupId>
	<artifactId>sparql-engine-neo</artifactId>
	<version>0.1-SNAPSHOT</version>
	<name>Sparql-engine for neo.</name>
	<description>A sparql-engine adapter for neo.</description>

	<repositories>
		<repository>
			<id>neo4j-public-repository</id>
			<name>Publically available Maven 2 repository for Neo4j</name>
			<url>http://m2.neo4j.org</url>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>
	</repositories>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
			</plugin>
		</plugins>
	</build>

	<dependencies>
		<dependency>
			<groupId>org.neo4j</groupId>
			<artifactId>graph-matching</artifactId>
			<version>0.1-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.1</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.neo4j</groupId>
			<artifactId>neo</artifactId>
			<version>1.0-b6-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>external</groupId>
			<artifactId>sparql-engine</artifactId>
			<version>0.8</version>
		</dependency>
		<dependency>
			<groupId>external</groupId>
			<artifactId>openrdf-model</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>external</groupId>
			<artifactId>openrdf-util</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>external</groupId>
			<artifactId>rio</artifactId>
			<version>1.0</version>
		</dependency>
	</dependencies>

</project>
